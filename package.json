{
  "name": "stahlferro.github.io",
  "version": "1.0.0",
  "description": "StahlFerro&#39;s website",
  "author": "StahlFerro",
  "private": true,
  "scripts": {
    "css-build": "node-sass --omit-source-map-url static/sass/main.scss static/css/main.css",
    "css-watch": "yarn run css-build --watch",
    "dev": "nuxt",
    "build": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "buildpage": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "genpage": "rimraf dist && cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn run lint",
    "push2master": "git subtree push --prefix dist origin master"
  },
  "dependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "nuxt": "^2.14.4",
    "vue-clipboard2": "^0.3.1"
  },
  "devDependencies": {
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.2.0",
    "babel-eslint": "^10.1.0",
    "bulma": "^0.9.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "webpack": "^4.44.1"
  }
}
